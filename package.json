{
	"name": "@rbxts/gamejoy",
	"version": "1.0.0",
	"description": "Input library",
	"main": "out/init.lua",
	"types": "out/index.d.ts",
	"scripts": {
		"build": "rbxtsc --verbose",
		"watch": "rbxtsc -w",
		"eslint": "eslint src tests --max-warnings 0",
		"test": "npm run build && npm run test:compile && npm run test:rojo && npm run test:run",
		"test:compile": "cd tests && rbxtsc --verbose",
		"test:rojo": "rojo build tests -o ./tests/test.rbxlx",
		"test:run": "run-in-roblox --place ./tests/test.rbxlx --script ./tests/out/testBootstrap.server.lua",
		"prepublishOnly": "npm test"
	},
	"keywords": [
		"Roblox",
		"Input"
	],
	"publishConfig": {
		"access": "public"
	},
	"author": "Rimuy",
	"license": "MIT",
	"files": [
		"out"
	],
	"devDependencies": {
		"@rbxts/compiler-types": "^1.2.7-types.0",
		"@rbxts/testez": "^0.3.1-ts.7",
		"@rbxts/types": "^1.0.549",
		"@typescript-eslint/eslint-plugin": "^5.3.1",
		"@typescript-eslint/parser": "^5.3.1",
		"eslint": "^8.2.0",
		"eslint-config-prettier": "^8.3.0",
		"eslint-plugin-prettier": "^4.0.0",
		"eslint-plugin-roblox-ts": "^0.0.32",
		"prettier": "^2.4.1",
		"roblox-ts": "^1.2.7",
		"typescript": "^4.4.4"
	},
	"dependencies": {
		"@rbxts/bin": "^1.2.1",
		"@rbxts/rust-classes": "^0.11.0-beta.1",
		"@rbxts/services": "^1.2.0",
		"@rbxts/signal": "^1.0.3",
		"@rbxts/t": "^2.2.1"
	}
}
